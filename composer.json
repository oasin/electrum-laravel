{
    "name": "oasin/electrum",
    "description": "Laravel Electrum Package",
    "keywords": [
        "oasin",
        "laravel",
        "electrum"
    ],
    "homepage": "https://github.com/oasin/electrum",
    "license": "MIT",
    "authors": [
        {
            "name": "oasin",
            "email": "31675779+oasin@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    
    "autoload": {
        "psr-4": {
            "Oasin\\Electrum\\": "src",
            "Oasin\\Electrum\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oasin\\Electrum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oasin\\Electrum\\ElectrumServiceProvider"
            ],
            "aliases": {
                "Electrum": "Oasin\\Electrum\\ElectrumFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
